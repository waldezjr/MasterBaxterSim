<?xml version="1.0"?>
<robot name="human_right_arm">

	<link name="base_link"/>
	<link name="link0"/>
	<link name="link1"/>
	<link name="link2"/>
	<link name="link3"/>
	<link name="link4"/>
	<link name="link5"/>
	<link name="link6"/>
	<link name="EEF"/>
<!-- Links are equivalent to DH standard convention -->
<!-- 
i    theta	d    a    alpha    offset
1	0	0	0	pi/2	pi/2
2	0	0	0	pi/2	pi/2
3	0	0	l_1	pi/2	pi
4	0	0	0	-pi/2	pi/2
5	0	-l_2	0	pi/2	0
6	0	0	0	-pi/2	-pi/2
7	0	0	0	0	0 
-->
	<!-- Arm relaxed, palm (z_6) is facing forward  -->


	<joint name="q1" type="revolute">
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<limit lower="-3.141592653589793" upper="3.141592653589793" effort="1000.0" velocity="3"/>
		<parent link="base_link"/>
		<child link="link0"/>
		<axis xyz="0 0 1"/>
	</joint>


	<joint name="q2" type="revolute">
		<origin xyz="0 0 0" rpy="1.5707963267948966 0 1.5707963267948966"/>
		<limit lower="-3.141592653589793" upper="3.141592653589793" effort="1000.0" velocity="3"/>
		<parent link="link0"/>
		<child link="link1"/>
		<axis xyz="0 0 1"/>
	</joint>

	<joint name="q3" type="revolute">
		<origin xyz="0 0 0" rpy="1.5707963267948966 0 1.5707963267948966"/>
		<limit lower="-3.141592653589793" upper="3.141592653589793" effort="1000.0" velocity="3"/>
		<parent link="link1"/>
		<child link="link2"/>
		<axis xyz="0 0 1"/>
	</joint>

	<joint name="q4" type="revolute">
		<origin xyz="-0.25 0 0" rpy="1.5707963267948966 0 3.141592653589793"/>
		<limit lower="-3.141592653589793" upper="3.141592653589793" effort="1000.0" velocity="3"/>
		<parent link="link2"/>
		<child link="link3"/>
		<axis xyz="0 0 1"/>
	</joint>

	<joint name="q5" type="revolute">
		<origin xyz="0 0 0" rpy="-1.5707963267948966 0 1.5707963267948966"/>
		<limit lower="-3.141592653589793" upper="3.141592653589793" effort="1000.0" velocity="3"/>
		<parent link="link3"/>
		<child link="link4"/>
		<axis xyz="0 0 1"/>
	</joint>

	<joint name="q6" type="revolute">
		<origin xyz="0 0 -0.5" rpy="1.5707963267948966 0 0"/>
		<limit lower="-3.141592653589793" upper="3.141592653589793" effort="1000.0" velocity="3"/>
		<parent link="link4"/>
		<child link="link5"/>
		<axis xyz="0 0 1"/>
	</joint>

	<joint name="q7" type="revolute">
		<origin xyz="0 0 0" rpy="-1.5707963267948966 0 -1.5707963267948966"/>
		<limit lower="-3.141592653589793" upper="3.141592653589793" effort="1000.0" velocity="3"/>
		<parent link="link5"/>
		<child link="link6"/>
		<axis xyz="0 0 1"/>
	</joint>

	<joint name="EEF" type="fixed">
		<origin xyz="0.125 0 0" rpy="0 0 0"/>
		<parent link="link6"/>
		<child link="EEF"/>
		<axis xyz="0 0 1"/>
	</joint>


</robot>